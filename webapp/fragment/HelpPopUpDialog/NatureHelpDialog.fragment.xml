<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Nature"
		class="sapUiPopupWithPadding"
		items="{path: 'contS>/GetNatureSet', filters : [ { path : 'Bname', operator : 'EQ', value1 : 'MOB17'} ]}"
		search="_handleValueHelpSearchNat"
		confirm="_handleValueHelpCloseNat"
		cancel="_handleValueHelpCloseNat">
		<StandardListItem
			title="{contS>Natureid}"
			description="{contS>Naturedesc}" />
	</SelectDialog>
</core:FragmentDefinition>